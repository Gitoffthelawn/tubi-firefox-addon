{
    "manifest_version": 2,
    "name": "tubi-add-ranking",
    "description": "an addon to add IMDB ranking to tubi",
    "version": "1.0",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "48": "icons/48.png",
            "32": "icons/32.png",
            "24": "icons/24.png",
            "16": "icons/16.png"
        }
    },
    "permissions": [
        "storage",
        "*://www.omdbapi.com/*",
        "*://*.duckduckgo.com/*",
        "activeTab",
        "contextMenus"
    ],
    "icons": {
        "48": "icons/48.png",
        "32": "icons/32.png",
        "24": "icons/24.png",
        "16": "icons/16.png"
    }
}